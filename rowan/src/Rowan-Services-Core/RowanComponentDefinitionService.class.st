Class {
	#name : 'RowanComponentDefinitionService',
	#superclass : 'RowanDefinitionService',
	#instVars : [
		'component'
	],
	#category : 'Rowan-Services-Core'
}

{ #category : 'accessing' }
RowanComponentDefinitionService >> component [
	^component
]

{ #category : 'accessing' }
RowanComponentDefinitionService >> component: object [
	component := object
]
